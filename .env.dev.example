# Local services
REDIS_URL=redis://localhost:6379/0
QDRANT_URL=http://localhost:6333
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_DB=lifelog
POSTGRES_USER=lifelog
